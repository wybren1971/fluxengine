comment: 'Acorn ADFS generic settings (32-bit formats)'
is_extension: true

image_writer {
	filename: "acornadfs.img"
	img {}
}

layout {
	tracks: 80
	sides: 2
	layoutdata {
		sector_size: 1024
		physical {
			start_sector: 0
		}
	}
}

decoder {
	ibm {
		trackdata {
			ignore_side_byte: true
		}
	}
}
