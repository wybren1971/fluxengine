comment: 'Japanese PC 1232kB 5.25"/3.5" 77-track 8-sector DSHD'

image_reader {
	filename: "ibm1232.img"
	img {}
}

image_writer {
	filename: "ibm1232.img"
	img {}
}

layout {
	tracks: 77
	sides: 2
	layoutdata {
		sector_size: 1024
		physical {
			start_sector: 1
			count: 8
		}
	}
}

encoder {
	ibm {
		trackdata {
			target_rotational_period_ms: 167
			target_clock_period_us: 2
		}
	}
}

decoder {
	ibm {}
}

drive {
	high_density: true
}

filesystem {
	fatfs {}
}


