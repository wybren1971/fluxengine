encoder {
	c64 {}
}

decoder {
	c64 {}
}

tpi: 48

filesystem {
	cbmfs {}
}

option {
	name: "35"
	comment: "35-track variant (default)"
	message: "using 35-track variant"
	
	config {
		layout {
			tracks: 35
